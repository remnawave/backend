    "compression": "^1.8.0",
    "country-code-emoji": "^2.3.0",
    "dayjs": "^1.11.13",
    "enhanced-ms": "^3.0.0",
    "grammy": "^1.32.0",
    "helmet": "^8.0.0",
    "husky": "9.0.11",
    "hygen": "^6.2.11",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nanoid": "3.3.8",
    "nest-winston": "^1.10.2",
    "nestjs-cls": "^5.4.0",
    "nestjs-zod": "4.3.1",
    "nunjucks": "^3.2.4",
    "passport": "0.7.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "4.0.1",
    "pkg-types": "^1.3.0",
    "pm2": "5.4.3",
