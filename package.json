    "@bull-board/nestjs": "^6.7.10",
    "@cjs-exporter/p-map": "7.0.2",
    "@grammyjs/parse-mode": "^1.10.0",
    "@kastov/grammy-nestjs": "0.4.1",
    "@nestjs-cls/transactional": "2.5.0",
    "@nestjs-cls/transactional-adapter-prisma": "1.2.16",
    "@nestjs/axios": "4.0.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "11.0.11",
    "@nestjs/config": "4.0.1",
    "@nestjs/core": "11.0.11",
    "@nestjs/cqrs": "11.0.2",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.11",
    "@nestjs/schedule": "5.0.1",
    "@nestjs/serve-static": "5.0.3",
    "@nestjs/swagger": "11.0.6",
    "@prisma/client": "6.4.1",
    "@remnawave/node-contract": "0.4.1",
    "@remnawave/xtls-sdk": "^0.2.0",
    "@scalar/nestjs-api-reference": "^0.4.0",
    "@supercharge/request-ip": "^1.2.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.41.7",
